{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "node": true,
    "amd": false
  },
  "rules": {
    "brace-style": [
      1
    ],
    "camelcase": [
      1
    ],
    "comma-dangle": [
      0
    ],
    "eol-last": [
      0
    ],
    "eqeqeq": [
      2,
      "allow-null"
    ],
    "guard-for-in": [
      1
    ],
    "indent": [
      2,
      4,
      {
        "SwitchCase": 1
      }
    ],
    "keyword-spacing": [
      1,
      {
        "before": true,
        "after": true
      }
    ],
    "new-cap": [
      0
    ],
    "no-bitwise": [
      1
    ],
    "no-caller": [
      0
    ],
    "no-delete-var": [
      1
    ],
    "no-empty": [
      1
    ],
    "no-eval": [
      2
    ],
    "no-inner-declarations": [
      1
    ],
    "no-loop-func": [
      0
    ],
    "no-mixed-requires": [
      0
    ],
    "no-mixed-spaces-and-tabs": [
      2
    ],
    "no-multi-spaces": [
      2
    ],
    "no-multiple-empty-lines": [
      "error",
      {
        "max": 1,
        "maxEOF": 0
      }
    ],
    "no-proto": [
      1
    ],
    "no-shadow": [
      1
    ],
    "no-trailing-spaces": [
      2
    ],
    "no-undef": [
      1
    ],
    "no-underscore-dangle": [
      0
    ],
    "no-unused-vars": [
      2,
      {
        "args": "none"
      }
    ],
    "one-var": [
      2,
      "never"
    ],
    "quotes": [
      1,
      "single"
    ],
    "semi": [
      2,
      "always"
    ],
    "wrap-iife": [
      2,
      "inside"
    ],
    "arrow-parens": [
      1,
      "as-needed"
    ],
    "arrow-spacing": [
      2,
      {
        "before": true,
        "after": true
      }
    ],
    "array-callback-return": [
      2
    ],
    "generator-star-spacing": [
      2,
      "after"
    ],
    "no-const-assign": [
      2
    ],
    "no-dupe-class-members": [
      2
    ],
    "no-duplicate-imports": [
      1
    ],
    "no-useless-constructor": [
      1
    ],
    "no-var": [
      1
    ],
    "object-shorthand": [
      1,
      "never"
    ],
    "prefer-arrow-callback": [
      2
    ],
    "prefer-const": [
      1
    ],
    "prefer-rest-params": [
      2
    ],
    "prefer-template": [
      2
    ],
    "template-curly-spacing": [
      1,
      "never"
    ]
  },
  "globals": {
    "_": true,
    "afterEach": true,
    "beforeEach": true,
    "describe": true,
    "expect": true,
    "it": true,
    "jasmine": true,
    "pending": true,
    "spyOn": true,
    "waits": true,
    "waitsFor": true,
    "xdescribe": true,
    "xit": true,
    "beforeAll": true,
    "afterAll": true,
    "runs": true
  }
}
